<h1>Welcome to Recipe Box!</h1>

<h3>All Recipes</h3>
<ul>
      <% @genres.each do |genre| %>
      <li> <%=genre.genre_name %> </li>
      <ul> <% genre.recipes.each do |recipe| %>
      <li><a href= "/recipes/<%=recipe.id%>"> <%= recipe.name %> </a></li> <br>
      <% end %>
      <% end %>
      </ul>
      </ul>
